{
    "name": "ersindemirtas/reviewston-php",
    "type": "metapackage",
    "description": "Reviewston multi reviews service library for 7.4+ library",
    "keywords": [
        "reviews",
        "trustpilot",
        "reviews.io"
    ],
    "authors": [
        {
            "name": "Ersin Demirtas",
            "email": "ed5@me.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.4",
        "php-http/mock-client": "^1.4",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ErsinDemirtas\\Reviewston\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErsinDemirtas\\Reviewston\\Tests\\" : "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
